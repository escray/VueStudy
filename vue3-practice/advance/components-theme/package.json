{
  "name": "standards",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:components && npm run build:dts && npm run build:css",
    "build:components": "vite-node ./scripts/build-module.ts",
    "build:dts": "vite-node ./scripts/build-dts.ts",
    "build:css": "vite-node ./scripts/build-css.ts",
    "dev:components": "vite-node ./scripts/dev-components.ts",
    "dev:business": "vite-node ./scripts/dev-business.ts",
    "dev:work-front": "vite-node ./scripts/dev-work-front.ts",
    "postinstall": "npm run build",
    "test": "vitest",
    "test:update": "vitest --update",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "build-vite": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/less": "^3.0.6",
    "@types/node": "^20.10.3",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.1.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "fast-glob": "^3.3.2",
    "jsdom": "^23.1.0",
    "less": "^4.2.0",
    "pinia": "^2.1.7",
    "prettier": "^3.1.0",
    "rollup": "^4.6.1",
    "rollup-plugin-esbuild": "^6.1.0",
    "ts-morph": "^21.0.1",
    "typescript": "^5.3.2",
    "unplugin-vue-macros": "^2.7.0",
    "vite": "^5.0.0",
    "vite-node": "^1.0.4",
    "vitest": "^1.1.3",
    "vue": "^3.3.11",
    "vue-tsc": "^1.8.22"
  }
}
